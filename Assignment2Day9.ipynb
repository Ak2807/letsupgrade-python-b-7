        "def getArmstrongnum():\n",
        "   for num in range(1,1000):  \n",
        "      sum = 0  \n",
        "      temp = num  \n",
        "      while temp > 0:  \n",
        "          digit = temp % 10  \n",
        "          sum += digit ** 3  \n",
        "          temp //= 10  \n",
        "          if num == sum:  \n",
        "              yield num  \n",
        "\n",
        "a=list ( getArmstrongnum () )\n",
        "print(a)"
      ],
